{
  "name": "nestjs-got-cjs",
  "version": "1.0.0",
  "description": "Got module for Nest framework (node.js) ðŸ—‚ | Cross-compatible module for NestJS and common JS, providing essential functionalities.",
  "keywords": [
    "nestjs got commonjs",
    "nestjs got cjs",
    "nestjs-got-cjs",
    "nestjs-got",
    "nestjs got",
    "nest-got",
    "got",
    "got-cjs",
    "nest http",
    "nestjs http"
  ],
  "homepage": "https://github.com/sevteen/nestjs-got-cjs#readme",
  "bugs": {
    "url": "https://github.com/sevteen/nestjs-got-cjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sevteen/nestjs-got-cjs.git"
  },
  "license": "MIT",
  "author": "Ilham Hanif",
  "main": "dist/src/index.ts",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "pnpm run prebuild && tsc --project tsconfig.build.json",
    "eslint:check": "eslint \"{src,test}/**/*.ts\"",
    "eslint:fix": "pnpm run eslint:check --fix",
    "lint": "pnpm run eslint:check && pnpm run prettier:check",
    "precommit": "lint-staged && pnpm run lint",
    "prepare": "husky",
    "prepush": "pnpm run test",
    "prettier:check": "prettier --check ./**/**/*.{ts,js,json}",
    "prettier:write": "prettier --write ./**/**/*.{ts,js,json}",
    "release": "semantic-release",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@eslint/compat": "1.1.1",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.8.0",
    "@ih.sevteen/tsconfig": "^1.0.0",
    "@nestjs/common": "^10.3.10",
    "@nestjs/core": "^10.3.10",
    "@nestjs/testing": "^10.3.10",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "15.9.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "jest-mock-extended": "^3.0.7",
    "lint-staged": "15.2.8",
    "nock": "^13.5.4",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "semantic-release": "24.0.0",
    "sort-package-json": "2.10.0",
    "stream-mock": "^2.0.5",
    "ts-jest": "^29.2.3",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.3.10",
    "got": "npm:got-cjs-compat@^14.4.5",
    "rxjs": "^7.8.1"
  },
  "engines": {
    "node": "^18.13.0 || ^20.9.0",
    "pnpm": ">=5.0.0"
  }
}
